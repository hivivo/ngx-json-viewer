{
  "name": "ngx-json-viewer",
  "version": "3.0.0",
  "description": "JSON formatter / viewer for Angular 2/4/5/6/7/8+",
  "keywords": [
    "angular",
    "json"
  ],
  "author": "Vivo Xu <vivo@fancyber.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hivivo/ngx-json-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/hivivo/ngx-json-viewer/issues"
  },
  "homepage": "https://github.com/hivivo/ngx-json-viewer#readme",
  "main": "index.ts",
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "pack:lib": "npm run build && npm pack ./dist",
    "publish:lib": "npm run build && npm publish ./dist"
  },
  "dependencies": {},
  "peerDependencies": {
    "@angular/common": ">=10.0.0",
    "@angular/core": ">=10.0.0",
    "rxjs": ">=6.6.2"
  },
  "devDependencies": {
    "@angular/common": "^11.0.7",
    "@angular/compiler": "^11.0.7",
    "@angular/compiler-cli": "^11.0.7",
    "@angular/core": "^11.0.7",
    "ng-packagr": "^11.0.3",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "typescript": "~4.0.2",
    "zone.js": "^0.10.2"
  }
}
